{
    "name": "public-ip-function",
    "version": "1.0.0",
    "description": "Public IP firebase function",
    "scripts": {
        "lint": "eslint .",
        "serve": "firebase emulators:start --only functions",
        "shell": "firebase functions:shell",
        "start": "npm run shell",
        "deploy-public": "firebase deploy --only functions:publicFunctions",
        "deploy-auth": "firebase deploy --only functions:authFunctions",
        "logs": "firebase functions:log",
        "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,yml,yaml,html,md}\""
    },
    "engines": {
        "node": "8"
    },
    "dependencies": {
        "firebase-admin": "^8.10.0",
        "firebase-functions": "^3.6.1"
    },
    "devDependencies": {
        "eslint": "^5.12.0",
        "eslint-plugin-promise": "^4.0.1",
        "firebase-functions-test": "^0.2.0",
        "prettier": "^2.0.5"
    },
    "private": true
}
